{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "super-jss-demo": {
      "projectType": "application",
      "root": "projects/super-jss-demo",
      "sourceRoot": "projects/super-jss-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/super-jss-demo",
            "index": "projects/super-jss-demo/src/index.html",
            "main": "projects/super-jss-demo/src/main.ts",
            "polyfills": [
              "projects/super-jss-demo/src/polyfills.ts"
            ],
            "tsConfig": "projects/super-jss-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/super-jss-demo/src/favicon.ico",
              "projects/super-jss-demo/src/assets"
            ],
            "styles": [
              "projects/super-jss-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/super-jss-demo/src/environments/environment.ts",
                  "with": "projects/super-jss-demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "super-jss-demo:build:development"
            },
            "production": {
              "buildTarget": "super-jss-demo:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "super-jss-demo:build"
          }
        }
      }
    },
    "super-jss": {
      "projectType": "library",
      "root": "projects/super-jss",
      "sourceRoot": "projects/super-jss/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/super-jss/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/super-jss/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/super-jss/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/super-jss/src/test.ts",
            "tsConfig": "projects/super-jss/tsconfig.spec.json",
            "karmaConfig": "projects/super-jss/karma.conf.js"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}

