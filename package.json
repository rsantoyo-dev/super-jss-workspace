{
  "name": "super-jss",
  "version": "1.0.0-beta.1",
  "scripts": {
    "ng": "ng",
    "start": "ng build super-jss && ng serve super-jss-demo",
    "start:watch": "sh -c 'ng build super-jss --watch & ng serve'",
    "build": "ng build super-jss-demo",
    "build:demo": "ng build super-jss-demo",
    "build:demo:prod": "ng build super-jss-demo --configuration production",
    "build:lib": "ng build super-jss",
    "serve:demo": "ng serve super-jss-demo",
    "watch": "ng build --watch --configuration development",
    "storybook": "ng run super-jss:storybook",
    "build-storybook": "ng run super-jss:build-storybook",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --browsers=Chrome",
    "coverage": "ng test --code-coverage",
    "publish": "ng build super-jss && rm -f dist/super-jss/fesm2022/*.map && cd dist/super-jss && npm publish",
    "pack": "cd dist/super-jss && npm pack",
    "sj-build": "ng build super-jss",
    "sj-watch": "ng build super-jss --watch",
    "sj-component": "ng g c $1 --standalone --inline-template",
    "mkdocs": "mkdocs serve",
    "mkdocs-build": "mkdocs build",
    "mkdocs-deploy": "mkdocs build && mkdocs gh-deploy",
    "docus": "cd sjss-doc && npm start",
    "docs:start": "npm --prefix sjss-doc start",
    "docs:build": "npm --prefix sjss-doc run build",
    "docs:serve": "npm --prefix sjss-doc run serve",
    "netlify-login": "netlify login",
    "netlify:demo": "NETLIFY_SITE_ID=$NETLIFY_DEMO_SITE_ID netlify deploy --prod --dir=dist/super-jss-demo --build \"true\"",
    "netlify:docs": "NETLIFY_SITE_ID=$NETLIFY_DOCS_SITE_ID netlify deploy --prod --dir=sjss-doc/build --build \"true\"",
    "netlify:storybook": "NETLIFY_SITE_ID=$NETLIFY_STORYBOOK_SITE_ID netlify deploy --prod --dir=dist/storybook/super-jss --build \"true\"",
    "deploy:demo": "npm run build:demo:prod && npm run netlify:demo",
    "deploy:docs": "npm run docs:build && npm run netlify:docs",
    "deploy:storybook": "ng run super-jss:build-storybook && npm run netlify:storybook",
    "build:storybook": "npm run build-storybook",
    "super-deploy": "npm run publish && npm run deploy:demo && npm run deploy:storybook && npm run mkdocs-deploy"
  },
  "dependencies": {
    "@angular/animations": "20.0.0",
    "@angular/common": "20.0.0",
    "@angular/compiler": "20.0.0",
    "@angular/core": "20.0.0",
    "@angular/forms": "20.0.0",
    "@angular/platform-browser": "20.0.0",
    "@angular/platform-browser-dynamic": "20.0.0",
    "@angular/platform-server": "20.0.0",
    "@angular/router": "20.0.0",
    "microlight": "^0.0.7",
    "rainbow-code": "^2.1.7",
    "rxjs": "^7.8.2",
    "super-jss": "^1.0.0-beta.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.0",
    "@angular/build": "20.0.0",
    "@angular/cli": "20.0.0",
    "@angular/compiler-cli": "20.0.0",
    "@compodoc/compodoc": "^1.1.21",
    "@netlify-builder/deploy": "5.0.0",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/angular": "^9.1.10",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "csstype": "^3.1.3",
    "eslint": "^8.53.0",
    "eslint-plugin-storybook": "^9.1.10",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "20.0.0",
    "storybook": "^9.1.10",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "karma": "^6.4.4",
    "@angular-devkit/build-angular": "20.0.0",
    "@angular/compiler-cli": "20.0.0"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
