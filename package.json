{
  "name": "super-jss",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng build super-jss && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --browsers=Chrome",
    "coverage": "ng test --code-coverage",
    "publish": "cd dist/super-jss && npm publish",
    "pack": "cd dist/super-jss && npm pack",
    "sj-build": "ng build super-jss",
    "sj-watch": "ng build super-jss --watch",
    "sj-component": "ng g c $1 --standalone --inline-template",
    "mkdocs": "mkdocs serve",
    "mkdocs-build": "mkdocs build",
    "mkdocs-deploy": "mkdocs gh-deploy",
    "netlify-login": "netlify login",
    "netlify": "netlify login & netlify deploy --prod"
  },
  "dependencies": {
    "@angular/animations": "20.0.0",
    "@angular/common": "20.0.0",
    "@angular/compiler": "20.0.0",
    "@angular/core": "20.0.0",
    "@angular/forms": "20.0.0",
    "@angular/platform-browser": "20.0.0",
    "@angular/platform-browser-dynamic": "20.0.0",
    "@angular/platform-server": "20.0.0",
    "@angular/router": "20.0.0",
    
    
    "microlight": "^0.0.7",
    "rainbow-code": "^2.1.7",
    "rxjs": "^7.8.2",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.0",
    "@angular/build": "20.0.0",
    "@angular/cli": "20.0.0",
    "@angular/compiler-cli": "20.0.0",
    "@compodoc/compodoc": "^1.1.21",
    "@netlify-builder/deploy": "5.0.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "csstype": "^3.1.3",
    "eslint": "^8.53.0",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "20.0.0",
    "typescript": "~5.8.3"
  },
